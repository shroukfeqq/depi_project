@model Match

<form action="/AdminPanel/saveEdit/@Model.Match_Id" method="Post">
    <div class="form-group">
        <label style="color:darkblue" for="Name">Match Name</label>
        <input type="text" class="form-control" id="Name" name="Name" value="@Model.Name" >
    </div>

    <div class="form-group">
        <label style="color:darkblue" for="TeamA">TeamA Name</label>
        <input type="text" class="form-control" id="TeamA" name="TeamA" value="@Model.TeamA" id="TeamA">
    </div>

    <div class="form-group">
        <label style="color:darkblue" class="form-control" for="TeamB">TeamB Name </label>

        <input type="text" class="form-control" name="TeamB" value="@Model.TeamB" id="TeamB">

    </div>

      <div class="form-group">
        <label style="color:darkblue" class="form-control" for="Date">Date</label>
        <input type="text" class="form-control" name="Date" id="Date" value="@Model.Date">

    </div>
    <div class="form-group">
        <label style="color:darkblue" class="form-control" for="Location">Location</label>
        <input type="text" class="form-control" name="Location" id="Location" value="@Model.Location">

    </div>

     <div class="form-group">
        <label style="color:darkblue" class="form-control" for="AvailableTickets">Available Tickets</label>
        <input type="text" class="form-control" name="AvailableTickets" id="AvailableTickets" value="@Model.AvailableTickets">

    </div>
    <button type="submit" class="btn btn-primary mt-3">Update</button>
</form>






    


@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}

